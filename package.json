{
  "name": "jdc-personal-site",
  "description": "I'm a full-stack web developer and I love creating modern digital experiences. Some tech I enjoy include HTML + CSS, Javascipt, React, GraphQL, and Node.js.",
  "version": "3.0.0",
  "license": "MIT",
  "author": {
    "name": "Jacob D. Castro",
    "email": "jacob@jdc.dev",
    "url": "https://jacobdcastro.com/"
  },
  "contributors": [
    {
      "name": "Jacob D. Castro",
      "email": "jacob@jdc.dev",
      "url": "https://jacobdcastro.com/"
    }
  ],
  "keywords": [
    "gatsby",
    "react",
    "static",
    "portfolio",
    "blog"
  ],
  "scripts": {
    "start": "gatsby develop --port 1414",
    "dev": "gatsby develop --port 1414 --open",
    "dev-host": "gatsby develop --port 1414 --host 0.0.0.0 --open",
    "build": "gatsby build --prefix-paths",
    "format": "prettier --write \"**/*.{js,json,md}\" \"!node_modules/**\" \"!public/**\"",
    "test": "jest --watchAll",
    "clean": "gatsby clean && rm -rf node_modules yarn.lock package-lock.json"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.2.5",
    "classnames": "^2.3.1",
    "gatsby": "^3.3.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^3.3.0",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-mdx": "^2.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-postcss": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "module-alias": "^2.2.2",
