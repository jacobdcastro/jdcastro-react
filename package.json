    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0"
  },
  "repository": {
